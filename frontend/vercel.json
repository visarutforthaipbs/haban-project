{
  "headers": [
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/dogs/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "facebookexternalhit"
        }
      ],
      "destination": "https://api.haban.love/api/dogs/:id?format=html"
    },
    {
      "source": "/dogs/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "Facebot"
        }
      ],
      "destination": "https://api.haban.love/api/dogs/:id?format=html"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
