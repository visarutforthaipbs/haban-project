{
  "headers": [
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/dogs/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*?(facebookexternalhit|Facebot|LinkedInBot|Twitterbot|WhatsApp|Line|Slackbot|TelegramBot|Discordbot|Pinterest).*"
        }
      ],
      "destination": "https://haban-project.onrender.com/preview/dogs/:id"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
